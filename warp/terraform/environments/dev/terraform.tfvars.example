# Example Terraform variables file for WARP Development Environment
# Copy this to terraform.tfvars and fill in your values

project_id = "your-gcp-project-id"
region     = "us-central1"

# For production, restrict these IPs
sip_allowed_ips = [
  "0.0.0.0/0"  # Allow all for development
]

# For production, restrict master access
master_authorized_networks = [
  {
    cidr_block   = "0.0.0.0/0"
    display_name = "All networks (dev only)"
  }
]