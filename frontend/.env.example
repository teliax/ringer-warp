# Frontend Environment Variables
# Copy to .env.local for local development

# API Configuration
VITE_API_URL=http://localhost:8080/v1
VITE_WS_URL=ws://localhost:8080/ws

# Auth Configuration
VITE_AUTH_DOMAIN=auth.warp.io
VITE_AUTH_CLIENT_ID=your_client_id_here

# Feature Flags
VITE_ENABLE_SMS=true
VITE_ENABLE_MMS=true
VITE_ENABLE_RCS=false
VITE_ENABLE_E911=true

# Analytics
VITE_GA_TRACKING_ID=
VITE_SENTRY_DSN=

# Public URLs
VITE_SUPPORT_URL=https://support.warp.io
VITE_DOCS_URL=https://docs.warp.io